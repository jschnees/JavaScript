<script language="JavaScript">
// takes a directory location, generates a random number and appends it to the directory
  function showImage(){
      var imgDir = 'photography/'; // image directory
      var max = 69; // max number of images in directory 
      for (count = 0; count < 5; count++) {
        // Runs 5 times, with values of count 0 through 4.
        var imgNum = Math.floor(Math.random() * 69) + 1; // generates a number between 1 and max each time 
        document.write('<img src="'+ imgDir + imgNum + 'jpg>"');
      }
  }
</script>
<div>
  <script language="JavaScript">
    showImage();
  </script>
</div>
